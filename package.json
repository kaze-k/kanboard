    "react-dom": "^18.0.0",
    "react-helmet-async": "^2.0.5",
    "react-router": "^7.3.0",
    "react-use": "^17.6.0",
    "reset-css": "^5.0.2",
    "screenfull": "^6.0.2",
    "sonner": "^2.0.1",
    "styled-components": "^6.1.16",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@ant-design/icons": "^5.6.1",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
